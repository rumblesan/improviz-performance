noFill()
i = sin(time/10) / 1
rotate(-0.5 * i, -0.5 * i, -0.3 * i)


func grid(width, height, &b)
  move(-width/2, -height/2, 0)
  	loop width times with x
  		loop height times with y
  			move(x, y, 0)
  				b()

loop 3 times with i
  move(0, 0, -0.3 * i)
    grid(20, 13)
      t = (time / 3) % 1
      move(t, 0, 0)
      rotate(0, 0, time/10)
        cube()
